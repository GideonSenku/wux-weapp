.wux-water-mark {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 2000;
  pointer-events: none;
  background-repeat: repeat;
  box-sizing: border-box
}
.wux-water-mark--full-page {
  position: fixed
}
.wux-water-mark__canvas {
  display: none;
  opacity: 0;
  visibility: hidden
}